    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@xmldom/xmldom": "^0.8.11",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.23",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
