    "date-fns": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "next": "15.1.0",
    "node-forge": "^1.3.3",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "soap": "^1.6.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
