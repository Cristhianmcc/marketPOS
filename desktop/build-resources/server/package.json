{
  "name": "monterrial-pos",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push --accept-data-loss && next build",
    "build:only": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:catalog": "tsx scripts/seedCatalog.ts",
    "db:reset": "prisma migrate reset --force",
    "seed:units": "npx ts-node scripts/seed-units-sunat.ts",
    "seed:categories": "npx ts-node scripts/seed-categories.ts --all",
    "images:update": "tsx scripts/updateProductImages.ts",
    "images:upload": "tsx scripts/uploadImagesToCloudinary.ts",
    "sunat:worker": "tsx src/worker/sunatWorker.ts",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:download-pg": "node electron/download-postgres.js",
    "electron:build": "npm run build && node electron/prepare-build.js && electron-builder --win --x64",
    "electron:pack": "electron-builder --dir",
    "desktop:dev": "npm run desktop:db && concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:3000 && cd desktop && npm run dev\"",
    "desktop:build": "cross-env DESKTOP_BUILD=1 npm run build:only && cd desktop && npm run build",
    "desktop:standalone": "cross-env DESKTOP_BUILD=1 npm run build:only",
    "desktop:db": "powershell -ExecutionPolicy Bypass -File scripts/desktop/db-init.ps1",
    "desktop:db:unix": "bash scripts/desktop/db-init.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.989.0",
    "@aws-sdk/s3-request-presigner": "^3.989.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@prisma/client": "^5.22.0",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22",
    "@types/node-forge": "^1.3.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@xmldom/xmldom": "^0.8.11",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.23",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "next": "15.1.0",
    "node-forge": "^1.3.3",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "soap": "^1.6.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "xml-crypto": "^6.1.2",
    "xmlbuilder2": "^4.0.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "wait-on": "^7.2.0"
  },
  "description": "Monterrial POS - Sistema de Punto de Venta",
  "author": "Monterrial"
}
